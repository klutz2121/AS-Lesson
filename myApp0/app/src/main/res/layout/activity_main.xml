<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/calc"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginTop="124dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="177dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="177dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="enter"
            android:ems="10"
            android:id="@+id/editText"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="177dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="177dp" android:layout_marginTop="193dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="85dp"
            android:layout_height="50dp"
            android:id="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/editText" android:layout_marginTop="104dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="177dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="177dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>